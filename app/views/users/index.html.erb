<h1>All Employees</h1>

<table class="table table-hover">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |u| %>
      <tr>
        <td><%= [u.first_name, u.last_name].join(" ") %></td>
        <td><%= u.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>
